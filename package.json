{
  "name": "hyperiot",
  "version": "2.2.9",
  "scripts": {
    "ng": "node_modules/.bin/ng",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "build-memory": "npm run ng-high-memory && ng build",
    "start": "ng serve --proxy-config proxy.config.json",
    "startLoc": "ng serve --proxy-config proxy-local.config.json",
    "startProd": "ng serve --prod --proxy-config proxy.config.json",
    "startNoAot": "ng serve --aot false --proxy-config proxy.config.json",
    "startWithCert": "ng serve --proxy-config proxy.config.json --ssl --ssl-key localhost.key --ssl-cert localhost.crt",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "npx compodoc -p src/tsconfig.app.json",
    "ngx-extractor": "ngx-extractor",
    "postinstall": "patch-package",
    "locl-extract": "npx locl extract -s=dist/**/*.js -f=xlf -o=src/locale/translations.xlf"
  },
  "private": true,
  "dependencies": {
    "@angular/material": "12.2.13",
    "@hyperiot/widgets": "1.0.23",
    "@types/d3": "^5.7.2",
    "convert-units": "^2.3.4",
    "crypto-js": "^3.1.9-1",
    "css-element-queries": "^1.2.2",
    "d3": "^5.14.2",
    "http-server": "^0.12.3",
    "map-stream": "0.0.7",
    "moment": "^2.29.1",
    "moment-precise-range-plugin": "^1.3.0",
    "ngx-color-picker": "^12.0.1",
    "ngx-cookie-service": "^12.0.3",
    "ngx-moment": "^6.0.2",
    "plotly.js": "1.49.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.16",
    "@angular/animations": "12.2.16",
    "@angular/cdk": "12.2.13",
    "@angular/cli": "12.2.16",
    "@angular/common": "12.2.16",
    "@angular/compiler": "12.2.16",
    "@angular/compiler-cli": "12.2.16",
    "@angular/core": "12.2.16",
    "@angular/forms": "12.2.16",
    "@angular/language-service": "12.2.16",
    "@compodoc/compodoc": "^1.1.10",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.2",
    "gulp": "^4.0.2",
    "gulp-json-editor": "^2.5.2",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "~6.3.20",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "patch-package": "^6.1.2",
    "protractor": "~7.0.0",
    "rxjs": "7.5.5",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "4.2.4",
    "xlf-merge": "^1.0.6"
  }
}
