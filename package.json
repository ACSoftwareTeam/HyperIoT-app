{
  "name": "hyperiot",
  "version": "1.1.0",
  "scripts": {
    "ng": "ng",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "build-memory": "npm run ng-high-memory && ng build",
    "start": "ng serve --proxy-config proxy.config.json",
    "startLoc": "ng serve --proxy-config proxy-local.config.json",
    "startProd": "ng serve --prod --proxy-config proxy.config.json",
    "startNoAot": "ng serve --aot false --proxy-config proxy.config.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "npx compodoc -p src/tsconfig.app.json",
    "ngx-extractor": "ngx-extractor",
    "postinstall": "patch-package",
    "locl-extract": "npx locl extract -s=dist/**/*.js -f=xlf -o=src/locale/translations.xlf"
  },
  "private": true,
  "dependencies": {
    "@hyperiot/widgets": "^0.3.3",
    "@types/d3": "^5.7.2",
    "@types/moment-precise-range-plugin": "^0.2.0",
    "angular-gridster2": "^9.1.13",
    "convert-units": "^2.3.4",
    "crypto-js": "^3.1.9-1",
    "css-element-queries": "^1.2.2",
    "d3": "^5.14.2",
    "http-server": "^0.12.3",
    "map-stream": "0.0.7",
    "moment-precise-range-plugin": "^1.3.0",
    "ng-starrating": "1.0.20",
    "ngx-color-picker": "^11.0.0",
    "ngx-cookie-service": "^3.1.3",
    "ngx-toastr": "^12.1.0",
    "plotly.js": "1.49.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "9.1.13",
    "@angular/compiler": "9.1.13",
    "@angular/compiler-cli": "9.1.13",
    "@angular/language-service": "9.1.13",
    "@compodoc/compodoc": "^1.1.10",
    "@locl/cli": "0.0.1-beta.6",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.2.1",
    "gulp": "^4.0.2",
    "gulp-json-editor": "^2.5.2",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "patch-package": "^6.1.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "xlf-merge": "^1.0.6"
  }
}
