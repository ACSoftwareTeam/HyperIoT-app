{
  "name": "hyperiot",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "build-memory": "npm run ng-high-memory && ng build",
    "start": "ng serve --proxy-config proxy.config.json",
    "startLoc": "ng serve --proxy-config proxy-local.config.json",
    "startProd": "ng serve --prod --proxy-config proxy.config.json",
    "startNoAot": "ng serve --aot false --proxy-config proxy.config.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "npx compodoc -p src/tsconfig.app.json",
    "ngx-extractor": "ngx-extractor",
    "postinstall": "patch-package",
    "locl-extract": "npx locl extract -s=dist/**/*.js -f=xlf -o=src/locale/translations.xlf"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/localize": "~9.1.0",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/pwa": "^0.803.17",
    "@angular/router": "~9.1.0",
    "@angular/service-worker": "~9.1.0",
    "@hyperiot/components": "0.1.14",
    "@hyperiot/core": "^0.6.14",
    "@hyperiot/widgets": "^0.1.30",
    "@types/d3": "^5.7.2",
    "@types/moment-precise-range-plugin": "^0.2.0",
    "angular-gridster2": "^9.1.0",
    "angular-plotly.js": "^2.0.0",
    "bootstrap": "^4.3.1",
    "convert-units": "^2.3.4",
    "cron-editor": "^2.1.8",
    "crypto-js": "^3.1.9-1",
    "css-element-queries": "^1.2.2",
    "d3": "^5.14.2",
    "hammerjs": "^2.0.8",
    "http-server": "^0.12.3",
    "idb": "^5.0.1",
    "map-stream": "0.0.7",
    "moment": "^2.24.0",
    "moment-precise-range-plugin": "^1.3.0",
    "ng-starrating": "1.0.20",
    "ngx-color-picker": "^11.0.0",
    "ngx-cookie-service": "^10.0.0",
    "ngx-mask": "^8.1.7",
    "ngx-moment": "^3.4.0",
    "ngx-toastr": "^13.2.0",
    "plotly": "^1.0.6",
    "rxjs": "~6.5.4",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@compodoc/compodoc": "^1.1.10",
    "@locl/cli": "0.0.1-beta.6",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.2.1",
    "gulp": "^4.0.2",
    "gulp-json-editor": "^2.5.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "patch-package": "^6.1.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "xlf-merge": "^1.0.6"
  }
}
